{
  "configuration"                   : {
    "version"                       : "0.1.0",
    "schema"                        : 1
  },
  "commands"                        : [
    {
      "enable"                      : true,
      "name"                        : "927-always_alive",
      "line"                        : "$USER1$/927-always_alive.sh"
    },
    {
      "enable"                      : true,
      "name"                        : "927-oci_list",
      "line"                        : "$USER1$/927-oci_list.sh -r $_SERVICERESOURCE$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_dhcp",
      "line"                        : "$USER1$/check_dhcp $ARG1$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_ftp",
      "line"                        : "$USER1$/check_ftp -H $HOSTADDRESS$ $ARG1$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_hpjd",
      "line"                        : "$USER1$/check_hpjd -H $HOSTADDRESS$ $ARG1$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_http",
      "line"                        : "$USER1$/check_http -I $HOSTADDRESS$ $ARG1$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_imap",
      "line"                        : "$USER1$/check_imap -H $HOSTADDRESS$ $ARG1$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_local_disk",
      "line"                        : "$USER1$/check_disk -w $ARG1$ -c $ARG2$ -p $ARG3$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_local_load",
      "line"                        : "$USER1$/check_load -w $ARG1$ -c $ARG2$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_local_mrtgtraf",
      "line"                        : "$USER1$/check_mrtgtraf -F $ARG1$ -a $ARG2$ -w $ARG3$ -c $ARG4$ -e $ARG5$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_local_procs",
      "line"                        : "$USER1$/check_procs -w $ARG1$ -c $ARG2$ -s $ARG3$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_local_swap",
      "line"                        : "$USER1$/check_swap -w $ARG1$ -c $ARG2$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_local_users",
      "line"                        : "$USER1$/check_users -w $ARG1$ -c $ARG2$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_nt",
      "line"                        : "$USER1$/check_nt -H $HOSTADDRESS$ -p 12489 -v $ARG1$ $ARG2$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_pop",
      "line"                        : "$USER1$/check_pop -H $HOSTADDRESS$ $ARG1$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_smtp",
      "line"                        : "$USER1$/check_smtp -H $HOSTADDRESS$ $ARG1$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_snmp",
      "line"                        : "$USER1$/check_snmp -H $HOSTADDRESS$ $ARG1$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_ssh",
      "line"                        : "$USER1$/check_ssh $ARG1$ $HOSTADDRESS$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_tcp",
      "line"                        : "$USER1$/check_tcp -H $HOSTADDRESS$ -p $ARG1$ $ARG2$"
    },
    {
      "enable"                      : true,
      "name"                        : "check_udp",
      "line"                        : "$USER1$/check_udp -H $HOSTADDRESS$ -p $ARG1$ $ARG2$"
    },
    {
      "enable"                      : true,
      "name"                        : "check-host-alive",
      "line"                        : "$USER1$/check_ping -H $HOSTADDRESS$ -w 3000.0,80% -c 5000.0,100% -p 5"
    },
    {
      "enable"                      : true,
      "name"                        : "notify-host-by-email",
      "line"                        : "/usr/bin/printf \"%b\" \"***** Naemon *****\\n\\nNotification Type: $NOTIFICATIONTYPE$\\nHost: $HOSTNAME$\\nState: $HOSTSTATE$\\nAddress: $HOSTADDRESS$\\nInfo: $HOSTOUTPUT$\\n\\nDate/Time: $LONGDATETIME$\\n\" | /usr/bin/mail -s \"** $NOTIFICATIONTYPE$ Host Alert: $HOSTNAME$ is $HOSTSTATE$ **\" $CONTACTEMAIL$"
    },
    {
      "enable"                      : true,
      "name"                        : "notify-service-by-email",
      "line"                        : "/usr/bin/printf \"%b\" \"***** Naemon *****\\n\\nNotification Type: $NOTIFICATIONTYPE$\\n\\nService: $SERVICEDESC$\\nHost: $HOSTALIAS$\\nAddress: $HOSTADDRESS$\\nState: $SERVICESTATE$\\n\\nDate/Time: $LONGDATETIME$\\n\\nAdditional Info:\\n\\n$SERVICEOUTPUT$\\n\" | /usr/bin/mail -s \"** $NOTIFICATIONTYPE$ Service Alert: $HOSTALIAS$/$SERVICEDESC$ is $SERVICESTATE$ **\" $CONTACTEMAIL$"
    },
    {
      "enable"                      : true,
      "name"                        : "process-host-perfdata",
      "line"                        : "/usr/bin/printf \"%b\" \"$LASTHOSTCHECK$\\t$HOSTNAME$\\t$HOSTSTATE$\\t$HOSTATTEMPT$\\t$HOSTSTATETYPE$\\t$HOSTEXECUTIONTIME$\\t$HOSTOUTPUT$\\t$HOSTPERFDATA$\\n\" >> /var/lib/naemon/host-perfdata.out"
    },
    {
      "enable"                      : true,
      "name"                        : "process-service-perfdata",
      "line"                        : "/usr/bin/printf \"%b\" \"$LASTSERVICECHECK$\\t$HOSTNAME$\\t$SERVICEDESC$\\t$SERVICESTATE$\\t$SERVICEATTEMPT$\\t$SERVICESTATETYPE$\\t$SERVICEEXECUTIONTIME$\\t$SERVICELATENCY$\\t$SERVICEOUTPUT$\\t$SERVICEPERFDATA$\\n\" >> /var/lib/naemon/service-perfdata.out"
    },
    {
      "enable"                      : true,
      "name"                        : "test",
      "line"                        : "$USER1$/test.sh"
    },
    {
      "enable"                      : true,
      "name"                        : "test2",
      "line"                        : "$USER1$/test2.sh"
    }
  ],
  "hostgroups"                      : [
    {
      "alias"                       : "Servers - Linux",
      "enable"                      : true,
      "name"                        : "linux-servers"
    },
    {
      "alias"                       : "Network - Printers",
      "enable"                      : true,
      "name"                        : "network-printers"
    },
    {
      "alias"                       : "Network - Routers",
      "enable"                      : true,
      "name"                        : "network-routers"
    },
    {
      "alias"                       : "OCI - Data",
      "enable"                      : true,
      "name"                        : "oci-data"
    }
  ],
  "hosts"                           : {
    "clouds"                        : [
      {
        "address"                   : null,
        "alias"                     : "oci-cloud",
        "enable"                    : true,
        "host_name"                 : "Oracle Cloud Infrastructure",
        "hostgroups"                : [
          {
            "enable"                  : true,
            "name"                    : "oci-data"
          },
          {
            "enable"                  : false,
            "name"                    : "test"
          }
        ],
        "settings"                  : {
            "cpus"                  : 1,
            "storage"               : [],
            "memory"                : 2048
        },
        "use"                       : "cloud-infrastructure"
      }
    ],
    "servers"                       : [
      {
        "address"                   : "192.168.1.250",
        "alias"                     : "mediaserver",
        "enable"                    : true,
        "host_name"                 : "Media Server",
        "hostgroups"                : [],
        "settings"                  : {
          "cpus"                    : 1,
          "storage"                 : [],
          "memory"                  : 2048
        },
        "use"                       : "linux-server"
      }
    ]
  }
}